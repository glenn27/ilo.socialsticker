<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="plone">

    <head>
        <metal:block fill-slot="column_two_slot" />
    </head>
<body>

<metal:main fill-slot="main">
    <tal:main-macro metal:define-macro="main">
      <h1 class="documentFirstHeading" tal:content="string:Upload Sticker" />
    <div id="content-core">
            <div id='pledge-logo' class='right'>
                  <!-- loaded image here -->
            </div>
            <div class="pledge-form">
                  <metal:block use-macro="context/@@ploneform-macros/titlelessform" />
                  <span id="current_url" tal:content="context/absolute_url" style='display: none'></span>
            </div>
    </div>
      <!-- Custom section goes here below the form -->

      

    </tal:main-macro>
</metal:main>

</body>
</html>
